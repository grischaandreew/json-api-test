{
    "name": "andreew/json-api-test",
    "description": "This headless Laravel application provides a JSONAPI conform REST API for editorial content.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "cloudcreativity/laravel-json-api": "^0.11",
        "dimsav/laravel-translatable": "^8.0",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~3.3",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "ramsey/uuid": "^3.7",
        "venturecraft/revisionable": "1.*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "cloudcreativity/json-api-testing": "^0.2.0",
        "codeception/codeception": "^2.3",
        "codeception/verify": "^1.0",
        "codeception/specify": "^1.0",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "repositories": [
      {
        "url": "https://packagist.org", "type": "composer"
      }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
